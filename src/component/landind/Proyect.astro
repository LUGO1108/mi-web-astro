---
import Proyectos from "../../config/proyectos.json"
---

<h2 class="text-3xl font-bold text-red-700 text-center my-10">proyect</h2>
<div class="grid md:grid-cols-3 gap-3 ">
    {
    
        Proyectos.map((proyecto)=>(
            <div class="bg-gray-800 rounded-2xl">
                <h2 class="font-bold text-2xl text-center my-6">{proyecto.title}</h2>
                <p class="text-center  p-6">{proyecto.description}</p>
            </div>
            
        ))
    }

</div>
